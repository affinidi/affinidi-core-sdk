  },
  "dependencies": {
    "@affinidi/vc-common": "^1.3.0",
    "@ahryman40k/ts-fhir-types": "^4.0.34",
    "ts-toolbelt": "^6.4.2",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@affinidi/eslint-config": "1.0.1",
    "@affinidi/prettier-config": "1.0.1",
