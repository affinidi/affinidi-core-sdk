    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-react-app": "6.0.0-next.77",
    "eslint-plugin-prettier": "^3.4.1",
    "jest": "^27.3.1",
    "jsonld": "^4.0.1",
    "license-checker": "25.0.1",
    "lodash.uniq": "^4.5.0",
    "prettier": "^2.4.1",
    "ts-jest": "^27.0.7",
    "typescript": "^4.3.2"
  },
  "jest": {
    "preset": "ts-jest",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "testTimeout": 30000
  }
