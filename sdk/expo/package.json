{
  "name": "@affinidi/wallet-expo-sdk",
  "version": "6.0.0-beta.4",
  "description": "SDK monorepo for affinity DID solution for Expo",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "npm run tsc",
    "buildDocs": "typedoc --out docs src",
    "start": "npm run build && node dist/index.js",
    "tsc": "tsc -p tsconfig-build.json",
    "lint": "eslint src/** test/**",
    "prepare": "npm run build",
    "test:only": "cross-env NODE_ENV=test mocha -r ts-node/register test/unit/index.test.ts -p ./tsconfig.json --timeout 55000",
    "test": "npm run lint && npm run test:only",
    "test:integration": "cross-env NODE_ENV=test mocha -r ts-node/register test/integration/index.test.ts -p ./tsconfig.json --timeout 115000",
    "test:coverage": "cross-env NODE_ENV=test nyc --reporter=lcov --reporter=text-summary mocha -r ts-node/register test/unit/index.test.ts test/integration/index.test.ts -p ./tsconfig.json --timeout 55000",
    "test:coverage:codeclimate": "cross-env NODE_ENV=test nyc --reporter=lcov --reporter=text-summary mocha -r ts-node/register test/unit/index.test.ts test/integration/index.test.ts -p ./tsconfig.json --timeout 55000 && $GITHUB_WORKSPACE/tmp/cc-test-reporter format-coverage -t lcov -o $GITHUB_WORKSPACE/tmp/codeclimate.expo.json coverage/lcov.info --add-prefix $(pwd | rev | cut -d'/' -f-2 | rev)",
    "checkLicenses": "license-checker --production --failOn 'AGPL 3.0;LGPL 2.1'"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/affinityproject/affinidi-core-sdk.git",
    "directory": "sdk/expo"
  },
  "keywords": [
    "expo",
    "sdk",
    "affinity",
    "network",
    "did"
  ],
  "author": "Denis Popov <denis@slatestudio.com>",
  "contributors": [
    "Roman Brazhnyk <roman@affinity-project.org>"
  ],
  "license": "ISC",
  "dependencies": {
    "@affinidi/affinity-metrics-lib": "^0.0.25",
    "@affinidi/common": "^2.0.0-beta.3",
    "@affinidi/wallet-core-sdk": "^6.0.0-beta.4",
    "assert": "^2.0.0",
    "buffer": "^5.6.0",
    "eccrypto-js": "^5.0.0",
    "events": "^3.1.0",
    "expo-random": "^8.1.0",
    "stream-browserify": "^3.0.0",
    "text-encoding": "^0.7.0"
  },
  "devDependencies": {
    "@affinidi/eslint-config": "1.0.1",
    "@affinidi/prettier-config": "1.0.1",
    "@types/chai": "4.2.12",
    "@types/mocha": "5.2.7",
    "@types/node": "13.13.15",
    "@types/randombytes": "2.0.0",
    "@types/sinon": "9.0.4",
    "@types/sinon-chai": "3.2.4",
    "@types/text-encoding": "^0.0.35",
    "@typescript-eslint/eslint-plugin": "3.8.0",
    "@typescript-eslint/parser": "3.8.0",
    "chai": "4.2.0",
    "cross-env": "^7.0.3",
    "crypto-random-string": "3.3.0",
    "dotenv": "^8.2.0",
    "eslint": "7.6.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-prettier": "3.1.4",
    "license-checker": "25.0.1",
    "mocha": "7.2.0",
    "nyc": "15.1.0",
    "prettier": "2.0.5",
    "sinon": "9.0.2",
    "sinon-chai": "3.5.0",
    "ts-node": "8.10.2",
    "typedoc": "0.16.11",
    "typescript": "^4.3.2"
  },
  "gitHead": "39b8ae64eb5e37ddae2f80438b320adaefae8ef6"
}
