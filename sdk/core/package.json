    "eslint": "7.6.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-prettier": "3.1.4",
    "jsonwebtoken": "^8.5.1",
    "license-checker": "25.0.1",
    "mocha": "7.2.0",
    "nock": "12.0.3",
    "nyc": "15.1.0",
    "prettier": "2.0.5",
    "sinon": "9.0.2",
    "supertest": "^6.0.1",
    "ts-node": "8.10.2",
    "typedoc": "0.16.11",
    "typescript": "^4.3.2"
  },
  "gitHead": "5420b34bac444d42d16b336a7ae95f47741d150c"
}
