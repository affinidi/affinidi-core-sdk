{
  "name": "@affinidi/url-resolver",
  "version": "1.0.0",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "description": "Url resolver for Affinidi services",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/affinityproject/affinidi-core-sdk.git",
    "directory": "common-libs/url-resolver"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "lib",
    "affinity",
    "common"
  ],
  "author": "Vlad Mitchenko <vladyslav.m@affinidi.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/affinityproject/affinidi-core-sdk/issues"
  },
  "homepage": "https://github.com/affinityproject/affinidi-core-sdk#readme",
  "scripts": {
    "build": "npm run tsc",
    "start": "npm run build && node dist/index.js",
    "tsc": "tsc -p tsconfig-build.json",
    "lint": "eslint src test",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "NODE_ENV=test mocha -r ts-node/register test/unit/index.test.ts -p ./tsconfig.json --timeout 8000",
    "test:integration": "NODE_ENV=test mocha -r ts-node/register test/integration/index.test.ts -p ./tsconfig.json --timeout 10000 --exit",
    "test:coverage": "NODE_ENV=test nyc --reporter=lcov mocha -r ts-node/register test/unit/index.test.ts -p ./tsconfig.json --timeout 8000",
    "test:coverage:codeclimate": "npm run test:coverage && [[ -s ./coverage/lcov.info ]] && $GITHUB_WORKSPACE/tmp/cc-test-reporter format-coverage -t lcov -o $GITHUB_WORKSPACE/tmp/codeclimate.common_li.json coverage/lcov.info --add-prefix $(pwd | rev | cut -d'/' -f-2 | rev) || echo \"lcov file is empty. There is no tests\"",
    "prepublishOnly": "npm run test && npm run lint",
    "checkLicenses": "license-checker --production --failOn 'AGPL 3.0;LGPL 2.1' --summary"
  },
  "devDependencies": {
    "@affinidi/eslint-config": "^2.0.0-beta.0",
    "@affinidi/prettier-config": "^2.0.0-beta.0",
    "@types/chai": "^4.2.22",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.11.6",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "chai": "^4.3.4",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "got": "^11.8.2",
    "license-checker": "^25.0.1",
    "mocha": "^9.1.3",
    "nyc": "^15.1.0",
    "prettier": "^2.4.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
  }
}
