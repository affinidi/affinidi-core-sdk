{
  "name": "@affinidi/affinidi-did-auth-lib",
  "version": "1.1.0",
  "description": "Affinidi Did Auth Helpers",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run tsc",
    "semantic-release": "semantic-release",
    "start": "npm run build && node dist/index.js",
    "tsc": "tsc -p tsconfig-build.json",
    "lint": "eslint src",
    "lint:fix": "eslint src/*.ts src/**/*.ts --fix",
    "test": "NODE_ENV=test mocha -r ts-node/register test/unit/index.ts -p ./tsconfig.json --timeout 55000 --exit",
    "test-and-lint": "npm run build && npm run lint && NODE_ENV=test mocha -r ts-node/register test/unit/index.ts -p ./tsconfig.json --timeout 10000 --exit",
    "test:integration": "NODE_ENV=test mocha -r ts-node/register test/integration/index.ts -p ./tsconfig.json --timeout 55000 --exit",
    "test:coverage": "NODE_ENV=test nyc --reporter=lcov mocha -r ts-node/register test/unit/index.ts -p ./tsconfig.json --timeout 55000 --exit",
    "test:coverage:codeclimate": "NODE_ENV=test nyc --reporter=lcov mocha -r ts-node/register test/unit/index.ts -p ./tsconfig.json --timeout 55000 --exit && $GITHUB_WORKSPACE/tmp/cc-test-reporter format-coverage -t lcov -o $GITHUB_WORKSPACE/tmp/codeclimate.core.json coverage/lcov.info",
    "checkLicenses": "license-checker --production --failOn 'AGPL 3.0;LGPL 2.1'",
    "prepare": "npm run build",
    "prepublishOnly": "npm run test && npm run lint"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/affinidi/foundational/affinidi-did-auth-lib.git"
  },
  "keywords": [
    "affinidi",
    "didAuth"
  ],
  "author": "Denis Popov <denys.p@affinidi.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://gitlab.com/affinidi/foundational/affinidi-did-auth-lib/issues"
  },
  "homepage": "https://gitlab.com/affinidi/foundational/affinidi-did-auth-lib#readme",
  "dependencies": {
    "@affinidi/common": "^2.0.0-beta.6",
    "bluebird": "^3.5.5",
    "did-resolver": "^2.0.1",
    "jsontokens": "^3.0.0",
    "lodash.get": "^4.4.2",
    "lodash.pick": "^4.4.0",
    "node-fetch": "^2.6.1"
  },
  "devDependencies": {
    "@affinidi/eslint-config": "1.0.1",
    "@affinidi/prettier-config": "1.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/gitlab": "^6.0.9",
    "@semantic-release/npm": "^7.0.10",
    "@types/chai": "4.2.12",
    "@types/lodash.get": "^4.4.6",
    "@types/mocha": "8.0.3",
    "@types/node": "13.13.16",
    "@types/node-fetch": "^2.5.8",
    "@typescript-eslint/eslint-plugin": "3.10.1",
    "@typescript-eslint/parser": "3.10.1",
    "chai": "4.2.0",
    "config": "^3.0.1",
    "dotenv": "^8.2.0",
    "eslint": "7.8.1",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-prettier": "3.1.4",
    "license-checker": "25.0.1",
    "mocha": "8.1.3",
    "nock": "^12.0.3",
    "nyc": "15.1.0",
    "prettier": "2.1.1",
    "semantic-release": "^17.4.2",
    "ts-node": "9.0.0",
    "ts-toolbelt": "^6.9.4",
    "typescript": "3.9.7"
  },
  "resolutions": {
    "ajv": "6.12.4"
  }
}
